#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
    int t;
    cin >> t;
    while(t--){
        int n;
        cin >> n;
        set<int> uniqueDays;
        for(int i = 0; i < n; i++){
            int day;
            cin >> day;
            uniqueDays.insert(day);
        }
        cout << uniqueDays.size() << endl;
    }
}
